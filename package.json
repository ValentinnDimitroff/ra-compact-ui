{
    "name": "ra-compact-ui",
    "version": "1.1.1",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "dependencies": {
        "classnames": "^2.2.6",
        "prop-types": "^15.7.2"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.x",
        "react": "^16.8.x",
        "react-admin": "^3.x",
        "react-dom": "^16.x"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-react": "^7.12.5",
        "@testing-library/jest-dom": "^5.11.5",
        "@testing-library/react": "^11.1.1",
        "babel-jest": "^26.6.3",
        "eslint": "^7.18.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^26.6.3",
        "prettier": "2.2.1"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/examples/demo/"
        ],
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$",
            "<root_dir>/src/"
        ]
    },
    "scripts": {
        "test": "jest",
        "lint": "eslint",
        "babel-transpile": "babel src -d dist",
        "build": "rimraf ./dist && yarn babel-transpile",
        "prepare": "yarn build",
        "prettier": "prettier  --config prettier.config.js --write ./src/",
        "start-demo": "cd examples/demo && rm -rf node_modules/ra-compact-ui && yarn --check-files && yarn start"
    },
    "repository": "https://github.com/ValentinnDimitroff/ra-compact-ui.git",
    "author": "Valentin Dimitroff",
    "license": "MIT"
}
